<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundedCornerImageView">

        <!-- Corner radius -->
        <attr name="cornerRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="DoubleTextView">

        <!-- Corner radius -->
        <attr name="altText" format="string" />
    </declare-styleable>
    
    
    <declare-styleable name="ShapeClipDrawable">
        <attr name="clipOrientation" format="integer" />
        <attr name="gravity" format="integer" />
        <attr name="drawable" format="reference|color" />
        <attr name="radius" format="float" />
    </declare-styleable>

</resources>